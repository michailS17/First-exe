#include<iostream>
#include<string.h>
#include<cstring>
using namespace std;
int main()
{
char name[11];

char id[6][11]={"ets1070/14","ets1043/14","ets1045/14","ets1030/14","ets0997/14"};
cout << "enter an id: ";
cin>>name[11];
for(int i=0; i<6; i++)
{
if(!strcmp (id[i], name))
{
    cout<<"your id is " << id[i];
    return 0;
        
}
}
cout<< "id cant be found";
return 0;
}